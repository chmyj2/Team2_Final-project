<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.team12.main.team1.store.Team1ProductMapper">

	<!-- 상품 리스트 가져오기 -->
	<select id="getProductList" resultType="com.team12.main.team1.store.Team1ProductDTO">
	
	
	</select>

	
	<!-- 상품등록 -->
	<insert id="productReg" parameterType="map">
		insert into team1Product_tbl values(
		team1Product_tbl_seq.nextval,#{product_category},1,#{product_title},#{product_title2},
		#{product_price}, #{product_stock}, #{product_thumnail}, #{product_info}, #{product_manufactor})
	</insert>
	
	

</mapper>